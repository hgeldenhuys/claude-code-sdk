{
  "name": "claude-code-sdk",
  "version": "0.1.0",
  "description": "SDK for tracking Claude Code changes, extending components, and plugin marketplace",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "sesh": "./bin/sesh.ts",
    "hooks": "./bin/hooks.ts",
    "hook-events": "./transcript-tui-rs/target/release/hook-events",
    "hook-events-tui": "./bin/hook-events-tui.ts",
    "agent-daemon": "./bin/agent-daemon.ts",
    "comms-audit": "./bin/comms-audit.ts",
    "comms-memo": "./bin/comms-memo.ts",
    "comms-paste": "./bin/comms-paste.ts",
    "comms": "./bin/comms.ts",
    "comms-uat": "./bin/comms-uat.ts",
    "comms-demo": "./bin/comms-demo.ts",
    "comms-dashboard": "./bin/comms-dashboard.ts",
    "comms-e2e-test": "./bin/comms-e2e-test.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tracker": {
      "import": "./dist/tracker/index.js",
      "types": "./dist/tracker/index.d.ts"
    },
    "./marketplace": {
      "import": "./dist/marketplace/index.js",
      "types": "./dist/marketplace/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./hooks/framework": {
      "import": "./dist/hooks/framework/index.js",
      "types": "./dist/hooks/framework/index.d.ts"
    }
  },
  "scripts": {
    "postinstall": "bun scripts/patch-blessed.ts",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bunx @biomejs/biome check src/",
    "lint:fix": "bunx @biomejs/biome check --write src/",
    "format": "bunx @biomejs/biome format --write src/",
    "typecheck": "bunx tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build",
    "docs": "bun src/cli/docs.ts",
    "docs:check": "bun src/cli/docs.ts check",
    "docs:fetch": "bun src/cli/docs.ts fetch",
    "docs:status": "bun src/cli/docs.ts status",
    "transcript": "transcript-tui-rs/target/release/transcript",
    "transcript:build": "cargo build --release --manifest-path transcript-tui-rs/Cargo.toml -p transcript-cli",
    "sesh": "bun bin/sesh.ts",
    "hooks": "bun bin/hooks.ts",
    "hook-events": "transcript-tui-rs/target/release/hook-events",
    "hook-events:build": "cargo build --release --manifest-path transcript-tui-rs/Cargo.toml -p hook-events-cli",
    "hook-events-tui": "bun bin/hook-events-tui.ts",
    "agent-daemon": "bun bin/agent-daemon.ts",
    "comms-audit": "bun bin/comms-audit.ts",
    "comms-memo": "bun bin/comms-memo.ts",
    "comms-paste": "bun bin/comms-paste.ts",
    "comms": "bun bin/comms.ts",
    "comms-uat": "bun bin/comms-uat.ts",
    "comms-demo": "bun bin/comms-demo.ts",
    "comms-dashboard": "bun bin/comms-dashboard.ts",
    "comms-e2e-test": "bun bin/comms-e2e-test.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "latest",
    "@types/diff": "^8.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist",
    "bin",
    "plugins",
    "docs",
    "transcript-tui-rs",
    "README.md"
  ],
  "keywords": [
    "claude-code",
    "sdk",
    "anthropic",
    "plugins",
    "marketplace",
    "hooks",
    "session-naming",
    "ai-tools"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@inquirer/prompts": "^8.1.0",
    "@types/blessed": "^0.1.27",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.6.2",
    "claude-hooks-sdk": "^0.8.6",
    "diff": "^8.0.3",
    "marked": "^17.0.1",
    "yaml": "^2.8.2"
  }
}
